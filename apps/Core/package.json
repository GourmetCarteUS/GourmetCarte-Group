{
    "name": "grouping-core",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "main": "./dist/www.js",
    "scripts": {
        "build": "tsoa spec-and-routes --basePath=/api/core && esbuild src/run.ts --bundle --outfile=./dist/index.cjs --platform=node",
        "lint": "tslint --project tsconfig.json --format stylish",
        "serve": "tsoa spec-and-routes --basePath=/api/core && node --loader ts-node/esm --experimental-specifier-resolution=node src/run.ts ",
        "dev": "yarn serve",
        "start": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
        "typeorm": "typeorm-ts-node-esm",
        "makemigration": "typeorm-ts-node-esm migration:generate ./src/migrations/migration -d ./src/app-data-source.ts",
        "migrate": "typeorm-ts-node-esm migration:run -d ./src/app-data-source.ts"
    },
    "dependencies": {
        "axios": "^1.2.2",
        "concurrently": "^8.2.1",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "dayjs": "^1.11.10",
        "debug": "~2.6.9",
        "dotenv": "^16.3.1",
        "express": "~4.16.1",
        "group-common": "*",
        "http-errors": "~1.6.3",
        "js-base64": "^3.7.5",
        "jsonwebtoken": "^9.0.2",
        "mariadb": "^3.2.2",
        "mongoose-paginate-ts": "^1.3.1",
        "morgan": "~1.9.1",
        "mysql2": "^3.6.5",
        "reflect-metadata": "^0.2.0",
        "swagger-ui-express": "^5.0.0",
        "tsoa": "^5.1.1",
        "typeorm": "^0.3.17",
        "wechat-backend-miniprogram": "^0.0.1"
    },
    "devDependencies": {
        "@types/http-status-codes": "^1.2.0",
        "@types/node": "^20.1.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/validator": "^13.11.7",
        "esbuild": "^0.19.9",
        "husky": "^8.0.3",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
